{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "dsa-playground": {
      "development": {
        "local": 3001
      },
      "production": {
        "fallback": "dsa-playground-three.vercel.app"
      },
      "routing": [
        {
          "group": "dsa",
          "paths": [
            "/dsa",
            "/dsa/:path*"
          ]
        }
      ]
    }
  }
}